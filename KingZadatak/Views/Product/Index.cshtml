@model List<Product>
<h1>Product list</h1>
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Name
        </th>
        <th>
            Price
        </th>
        <th>
            Description
        </th>
        <th>
            Image
        </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u => u.Id))
        {
            <tr>
                <td>@obj.Title</td>
                <td>@obj.Price</td>
                <td>@obj.Description</td>
                <td><img src="@obj.Thumbnail" style="width:150px; height:150px" /></td>
                </tr>
        }
    </tbody>
</table>